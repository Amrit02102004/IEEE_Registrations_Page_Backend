{
  "version": 2,
  "routes": [
    { "src": "/check_user", "dest": "/api/server.js" }
  ],
  "rewrites": [
    {
      "source": "/get_domains/:email",
      "destination": "/api/server.js"
    },
    {
      "source": "/put_domains/:email",
      "destination": "/api/server.js"
    },
    {
      "source": "/profile/:email",
      "destination": "/api/server.js"
    }
  ]
}
